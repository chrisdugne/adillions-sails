<!-- BEGIN GoogleAnalytics -->
<script type="text/javascript">
  var _gaq = _gaq || [];
  _gaq.push(['_setDomainName', '{{ga.domainName}}']);
  _gaq.push(['_setAllowLinker', {{ga.allowLinker}}]);
  _gaq.push(['_setAllowHash', {{ga.allowHash}}]);
  _gaq.push(['_setCampaignCookieTimeout', {{ga.cookieTimeout}}]);
  _gaq.push(['_trackPageLoadTime']);
  {{#if ga.processURL}}
    _gaq.push(['_trackPageview', '{{{ga.processURL}}}']);
  {{else}}
    _gaq.push(['_trackPageview']);
  {{/if}}
  (function() {
   var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
   ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
   var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
</script>
<!-- END GoogleAnalytics -->
