<div class="team">

  <div class="page-header">
    <h1>{{i18n 'about_Team'}}</h1>
  </div>

  <p class="lead">{{i18n 'about_TeamIntro'}}</p>

  <div class="row">
    <div class="media">
      <div class="col-xs-12 col-md-3">
        <img src="/images/team/Timothee_de-COURLON.jpg" alt="{{i18n 'about_CEOName'}}" class="img-circle well mbs" width="200" height="200">
      </div>
      <div class="media-body col-xs-12 col-md-9">
        <h2>{{i18n 'about_ceo_name'}}</h2>
        <ul class="list-inline">
          <li>
            <a href="{{extUrl 'ceo_twitter'}}" class="social social-twitter" data-toggle="tooltip" data-placement="top" title="Twitter">
              <span class="hidden">Twitter</span>
            </a>
          </li>
          <li>
            <a href="{{extUrl 'ceo_googlePlus'}}" class="social social-google_plus" data-toggle="tooltip" data-placement="top" title="Google plus">
              <span class="hidden">Google plus</span>
            </a>
          </li>
        </ul>
        <h3>
          {{i18n 'about_cofounder'}}
          <small>{{i18n 'about_ceo'}}</small>
        </h3>
        <p>{{i18n 'about_ceo_desc'}}</p>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="media">
      <div class="col-xs-12 col-md-3">
        <img src="/images/team/Christophe_DUGNE-ESQUEVIN.jpg" alt="{{i18n 'about_cto_name'}}" class="img-circle well mbs" width="200" height="200">
      </div>
      <div class="media-body col-xs-12 col-md-9">
        <h2>{{i18n 'about_cto_name'}}</h2>
        <ul class="list-inline">
          <li>
            <a href="{{extUrl 'cto_twitter'}}" class="social social-twitter" data-toggle="tooltip" data-placement="top" title="Twitter">
              <span class="hidden">Twitter</span>
            </a>
          </li>
          <li><a href="{{extUrl 'cto_googlePlus'}}" class="social social-google_plus" data-toggle="tooltip" data-placement="top" title="Google plus">
            <span class="hidden">Google plus</span></a></li>
          <li>
            <a href="{{extUrl 'cto_github'}}" class="social social-github" data-toggle="tooltip" data-placement="top" title="Github">
              <span class="hidden">Github</span>
            </a>
          </li>
        </ul>
        <h3>
          {{i18n 'about_cofounder'}}
          <small>{{i18n 'about_cto'}}</small>
        </h3>
        <p>{{i18n 'about_cto_desc_1'}} <a href="http://www.maperial.com" target="_blank">Maperial</a>{{i18n 'about_cto_desc_2'}}</p>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="media">
      <div class="col-xs-12 col-md-3">
        <img src="/images/team/Antoine_DUVILLIER.jpg" alt="{{i18n 'about_fel_name'}}" class="img-circle well mbs" width="200" height="200">
      </div>
      <div class="media-body col-xs-12 col-md-9">
        <h2>{{i18n 'about_fel_name'}}</h2>
        <ul class="list-inline">
          <li>
            <a href="{{extUrl 'fel_twitter'}}" class="social social-twitter" data-toggle="tooltip" data-placement="top" title="Twitter">
              <span class="hidden">Twitter</span>
            </a>
          </li>
          <li>
            <a href="{{extUrl 'fel_googlePlus'}}" class="social social-google_plus" data-toggle="tooltip" data-placement="top" title="Google plus">
              <span class="hidden">Google plus</span>
            </a>
          </li>
          <li>
            <a href="{{extUrl 'fel_github'}}" class="social social-github" data-toggle="tooltip" data-placement="top" title="Github">
              <span class="hidden">Github</span>
            </a>
          </li>
        </ul>
        <h3>
          {{i18n 'about_cofounder'}}
          <small>{{i18n 'about_fel'}}</small>
        </h3>
        <p>{{{i18n 'about_fel_desc'}}}</p>
      </div>
    </div>
  </div>

</div>
